{
  "sinks": {
    "stdout": {
      "renderWith": "RENDER_ECS",
      "sendTo": "STDOUT"
    },
    "seq": {
      "seqServer": "http://localhost:5341"
    }
  },
  "logging": [
    {
      "levelRanges": [
        {
          "fromMinLevel": "INFO",
          "toMaxLevel": "ERROR",
          "stopOnMatch": true,
          "toSinks": [
            "stdout",
            "seq"
          ]
        }
      ]
    }
  ]
}